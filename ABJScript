if game.PlaceId == 5233268561 then
wait(1.5)
game.StarterGui:SetCore("SendNotification", {
    Title = "Nex Hub";
    Text = "A Bizzare Journey";
    Icon = "None";
    Duration = "0.5";
})
wait(2.5)
game.StarterGui:SetCore("SendNotification", {
    Title = "Loading Script Error!";
    Text = "Checking problem..";
    Icon = "None";
    Duration = "0.5";
})
wait(3)
game.StarterGui:SetCore("SendNotification", {
    Title = "Nex Hub";
    Text = "Script will no longer work";
    Icon = "None";
    Duration = "0.5";
})
wait()
game.StarterGui:SetCore("SendNotification", {
    Title = "Nex Hub";
    Text = "Sorry. Thanks for using nex hub!";
    Icon = "None";
    Duration = "0.5";
})
wait()
game.Players.LocalPlayer:kick("Script no longer works owner nub script will comeback if he wants to. Thanks for using Nex Hub!")
